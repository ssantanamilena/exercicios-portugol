algoritmo "DIVISAONUMREAIS"
var
   a , b, divisao : real
inicio
    escreval("Seja Bem vindo")
    escreval("-----------------------")
    escreval("Digite o primeiro valor: ")
    leia(a)
    escreval("Digite o segundo valor:")
    leia(b)
    divisao <- a / b
    escreval("O valor de" ,a, " dividido por" ,b, " é igual a" ,divisao, ".")
fimalgoritmo